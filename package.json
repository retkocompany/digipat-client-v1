    "electron": "^11.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "electron-alert": "^0.1.16",
    "electron-builder": "^22.11.7",
    "electron-forge": "^5.2.4",
    "electron-json-config": "^1.5.3",
    "electron-router": "^0.5.1",
    "electron-squirrel-startup": "^1.0.0",
    "filesaver": "^0.0.13",
    "filesaver.js": "^1.3.4",
